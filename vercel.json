{
	"functions": {
		"api/server.js": {
			"memory": 1024,
			"includeFiles": "db.json"
		}
	},
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "api/server.js"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "no-store, no-cache, must-revalidate, proxy-revalidate, max-age=0"
				}
			]
		}
	]
}
